{
    "compilerOptions": {
      "target": "ES5",
      "jsx": "preserve",
      "module": "commonjs",
      "allowJs": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "exclude": [
      "client",
      "dist",
      "node_modules"
    ],
    "include": [
      "src"
    ],
  }